    "orbit-db-kvstore": "0.1.2",
    "orbit-db-pubsub": "0.0.3"
  },
  "devDependencies": {
    "asyncawait": "^1.0.6",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
